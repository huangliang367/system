{
    "version": 5,
    "cmakeMinimumRequired": {
      "major": 3,
      "minor": 23,
      "patch": 0
    },
    "configurePresets": [
      {
        "name": "debug",
        "displayName": "System Debug Config",
        "description": "Default build using Unix generator",
        "generator": "Unix Makefiles",
        "binaryDir": "${sourceDir}/build/debug",
        "cacheVariables": {
            "CMAKE_BUILD_TYPE": "Debug",
            "TEST_VAL" : "TEST",
            "CMAKE_TOOLCHAIN_FILE": "${sourceDir}/toolchain/toolchain.cmake"
        }
      }
    ]
  }